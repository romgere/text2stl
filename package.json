    "lint:js": "eslint . --ext .js,.ts",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "generate:snapshot": "ts-node -r esm tests/fixtures/script/generate.ts"
